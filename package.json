{
  "name": "laita",
  "version": "0.1.2",
  "description": "A CLI utility to deploy static websites to the cloud",
  "author": "Viljami Kuosmanen",
  "license": "MIT",
  "homepage": "https://github.com/anttiviljami/laita#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anttiviljami/laita.git"
  },
  "bugs": {
    "url": "https://github.com/anttiviljami/laita/issues"
  },
  "keywords": [
    "static",
    "website",
    "hosting",
    "cloud",
    "aws",
    "s3",
    "cloudfront",
    "azure",
    "deploy"
  ],
  "bin": {
    "laita": "bin/laita"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch-build": "tsc -w",
    "lint": "eslint src/**/*.ts",
    "prettier": "prettier --write src/**",
    "prepare": "npm run build",
    "test": "NODE_ENV=test jest"
  },
  "files": [
    "laita",
    "*.js",
    "*.d.ts",
    "*.js.map",
    "**/*.js",
    "**/*.d.ts",
    "**/*.js.map",
    "terraform/**/*",
    "!src",
    "!*.test.*",
    "!*.config.js",
    "!node_modules",
    "!public",
    "!.laita*"
  ],
  "devDependencies": {
    "@types/ejs": "^2.6.3",
    "@types/fs-extra": "^8.0.0",
    "@types/inquirer": "^6.0.3",
    "@types/jest": "^24.0.15",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.0.10",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "ejs": "^2.6.2",
    "fs-extra": "^8.1.0",
    "inquirer": "^6.4.1",
    "js-yaml": "^3.13.1",
    "yargs": "^13.2.4"
  }
}
